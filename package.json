{
    "name": "soilcloud.socket",
    "version": "0.1.0",
    "private": true,
    "main": "build/index.js",
    "scripts": {
        "start": "NODE_ENV=production node --harmony build/index.js",
        "dev": "npm run tw:build && npm run nodemon",
        "nodemon": "NODE_ENV=development nodemon src/index.ts",
        "lint": "eslint src --fix",
        "clean": "rm -rf build",
        "prebuild": "npm run clean",
        "build": "npm run tw:build && npm run tsc:check && npm run tsc:build",
        "tsc:check": "tsc --noEmit -p tsconfig.json --composite false",
        "tsc:build": "NODE_ENV=production tsc -p tsconfig.json",
        "tw:watch": "npx tailwindcss -i src/index.css -o public/index.css --watch",
        "tw:build": "npx tailwindcss -i src/index.css -o public/index.css"
    },
    "dependencies": {
        "@tailwindcss/cli": "^4.1.17",
        "@tailwindcss/postcss": "^4.1.17",
        "@tsconfig/node-lts": "^22.0.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.7",
        "@typescript-eslint/eslint-plugin": "^8.20.0",
        "@typescript-eslint/parser": "^8.20.0",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.7",
        "eslint": "^9.18.0",
        "express": "^4.21.2",
        "socket.io": "^4.8.1",
        "tailwindcss": "^4.1.17",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3"
    }
}